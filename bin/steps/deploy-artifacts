#!/usr/bin/env bash

# These files will be checked separately.
# shellcheck disable=SC1091
. bin/lib/log.sh
. bin/lib/util.sh

for file do
  notice "processing file %s" "$file"

  if [[ ! -r $file ]]; then
    error "unable to load file %s" "$file"
    continue
  fi

  try aws s3 cp "$file" s3://"${S3_BKT_DST-aws-srv-doc-bkt}"/"${file##*/}"
done
