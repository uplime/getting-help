#!/usr/bin/env bash

# This file will be checked separately
# shellcheck disable=SC1091
. bin/lib/toolbox.sh

install aws

for file do
  notice "processing file %s" "$file"

  if [[ ! -r $file ]]; then
    error "unable to load file %s" "$file"
    continue
  fi

  aws s3 cp "$file" s3://"${S3_BKT_DST:-cloudops-docs}"/"$file"
done
